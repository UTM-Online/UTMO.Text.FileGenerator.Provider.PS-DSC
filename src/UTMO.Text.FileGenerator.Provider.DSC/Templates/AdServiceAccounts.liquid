{% extends "configuration_base" %}
{% block config %}
{% for msa in managed_service_accounts %}
    ADManagedServiceAccount "{{ msa.DscDisplayName }}"
    {
    Ensure = {{ msa.Ensure }}
    ServiceAccountName = '{{ msa.AccountName }}'
    AccountType = 'Group'
    ManagedPasswordPrincipals = @({{ msa.ManagingPrinciples }})
    }
{% endfor %}
{% endblock %}